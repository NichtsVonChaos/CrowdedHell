* {
        font-family: "Century Gothic";
}

QMainWindow {
        background: rgb(235, 240, 250);
}

QMenuBar {
        border-left: 2px solid #12244C;
        border-bottom: none;
        border-right: 2px solid #12244C;
        background: rgb(43, 87, 154); 
        font-size: 16px;
}
QMenuBar::item {
        padding: 3px 20px 3px 20px; 
        background: transparent;
}
QMenuBar::item:enabled {
        color: rgb(202, 216, 240);
}
QMenuBar::item:enabled:selected {
        background: rgba(255, 255, 255, 40);
}

QTabWidget {
        border: none; 
        background: rgb(202, 216, 240);
} 
QTabBar::tab {
        width: 92;
        height: 32;
        background: rgb(43, 87, 154);
        border-left: 2px solid #12244C;
        border-top: none;
        border-bottom: 2px solid #12244C;
        border-right: 2px solid #12244C;
        color:rgb(227, 234, 242);
        padding: 2px
}
QTabBar::tab:selected {
        background: rgb(202, 216, 240);
        border-top: 2px solid #12244C;
        border-bottom: none;
        color:rgb(35, 71, 124);
}
QTabWidget > QWidget {
        border-top: none;
        border-left: 2px solid #12244C;
        border-bottom: 2px solid #12244C;
        border-right: 2px solid #12244C;
        background: rgb(202, 216, 240);
}
QScrollArea {
        border: none;
}
QWidget#scrollAreaWidgetContents {
        border: none;
        background-color: rgb(202, 216, 240);
}

ResourceTreeView {
        border-top: none;
        border-left: 2px solid #12244C;
        border-bottom: 2px solid #12244C;
        border-right: 2px solid #12244C;
        background-color: rgb(202, 216, 240);
        color: rgb(30, 60, 104);
        show-decoration-selected: 1;
}
QHeaderView {
        font-size: 14px;
}
QHeaderView::section {
        background: #12244C;
        color: rgb(202, 216, 240);
        height: 25px;
}
ResourceTreeView::item:hover,
ResourceTreeView::branch:hover {
        background-color: rgb(134, 167, 221);
}
ResourceTreeView::item:selected,
ResourceTreeView::branch:selected {
        color: rgb(202, 216, 240);
        background-color: rgb(43, 87, 154);
}
ResourceTreeView::branch:has-siblings:!adjoins-item {
        border-image: url(:/Themes/Deep Blue/Icons/Tree-Vline.png) 0;
}
ResourceTreeView::branch:has-siblings:adjoins-item {
        border-image: url(:/Themes/Deep Blue/Icons/Tree-Branch-More.png) 0;
}
ResourceTreeView::branch:!has-children:!has-siblings:adjoins-item {
        border-image: url(:/Themes/Deep Blue/Icons/Tree-Branch-End.png) 0;
}
ResourceTreeView::branch:has-children:!has-siblings:closed,
ResourceTreeView::branch:closed:has-children:has-siblings {
        border-image: none;
        image: url(:/Themes/Deep Blue/Icons/Tree-Branch-Closed.png);
}
ResourceTreeView::branch:open:has-children:!has-siblings,
ResourceTreeView::branch:open:has-children:has-siblings  {
        border-image: none;
        image: url(:/Themes/Deep Blue/Icons/Tree-Branch-Open.png);
}

AudioPlayerSlider::groove:horizontal {
        image: url(:/Themes/Deep Blue/Icons/Slider-Bar.png);
}
AudioPlayerSlider::handle:horizontal {
        image: url(:/Themes/Deep Blue/Icons/Slider-Button.png);
}

QSlider#volumeSliderBar::groove:horizontal {
        image: url(:/Themes/Deep Blue/Icons/Volume-Bar.png);
}
QSlider#volumeSliderBar::handle:horizontal {
        image: url(:/Themes/Deep Blue/Icons/Volume-Button.png);
}

QGraphicsView {
        border: none;
        border-top: 2px solid #12244C;
        border-bottom: 2px solid #12244C;
}

QLineEdit {
        border-radius: 4px;
        border-top: 1px solid #12244C;
        border-left: 1px solid #12244C;
        border-bottom: 1px solid #12244C;
        border-right: 1px solid #12244C;
}
QLineEdit:read-only {
        background: #CCCCCC;
        color: #777777;
}

QTextEdit#textEditMessageBox {
        border: none;
        border-top: 2px solid #12244C;
        border-bottom: 2px solid #12244C;
        background: rgb(202, 216, 240);
}

QLabel {
        color: rgb(18, 36, 76);
}

QComboBox {
        border: 1px solid #12244C;
        border-radius: 4px;
}
QComboBox::drop-down {
        subcontrol-origin: padding;
        subcontrol-position: top right;
        width: 20px;
        border: none;
        border-right: 1px solid #12244C;
        border-top-left-radius: 4px;
        border-bottom-left-radius: 4px;
}
QComboBox::down-arrow {
        image: url(":/Themes/Deep Blue/Icons/comboBox.png");
}

